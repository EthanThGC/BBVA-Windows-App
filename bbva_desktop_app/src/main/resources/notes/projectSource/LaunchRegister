		lbl_add_additional_email = new JLabel();
		lbl_add_additional_email.addMouseListener(new MouseAdapter() {
			@Override
			public void mouseEntered(MouseEvent e) {
				
				lbl_add_additional_email.setCursor(new Cursor(Cursor.HAND_CURSOR));
				lbl_add_additional_email.setForeground(foregroundTextStandard);
				
				if(lbl_add_additional_email.getIcon().equals(icon.ADDITIONAL_EMAIL_EXITED)) {
					lbl_add_additional_email.setIcon(icon.ADDITIONAL_EMAIL_ENTERED);
				}else {
					if(lbl_add_additional_email.getIcon().equals(icon.UPDATE_ADDITIONAL_EMAIL_EXITED)) {
						lbl_add_additional_email.setIcon(icon.UPDATE_ADDITIONAL_EMAIL_ENTERED);
					}
				}
			}
			@Override
			public void mouseExited(MouseEvent e) {
				
				lbl_add_additional_email.setCursor(new Cursor(Cursor.DEFAULT_CURSOR));
				lbl_add_additional_email.setForeground(new Color(208, 211, 212));
				
				if(lbl_add_additional_email.getIcon().equals(icon.ADDITIONAL_EMAIL_ENTERED)) {
					lbl_add_additional_email.setIcon(icon.ADDITIONAL_EMAIL_EXITED);
				}else {
					if(lbl_add_additional_email.getIcon().equals(icon.UPDATE_ADDITIONAL_EMAIL_ENTERED)) {
						lbl_add_additional_email.setIcon(icon.UPDATE_ADDITIONAL_EMAIL_EXITED);
					}
				}
			}
			@Override
			public void mouseClicked(MouseEvent e) {
				
				if(additionalEmail.getAdditionalEmail() == null) additionalEmail.setVisible(true);
				
				additionalEmail.addWindowListener(new WindowAdapter() {
					@Override
					public void windowClosing(WindowEvent e) {
						if(additionalEmail.getAdditionalEmail() != null) updateAdditionalEmail();
					}
					@Override
					public void windowClosed(WindowEvent e) {
						if(additionalEmail.getAdditionalEmail() != null) updateAdditionalEmail();
					}
				});
			}
		});
