		btn_move_right.add(lbl_icon_move_right);
		
		lbl_text_left = new JLabel();
		lbl_text_left.setBounds(0, 0, 120, 40);
		lbl_text_left.setIcon(icon.arrow_left_exited);
		lbl_text_left.setText("Regresar");
		lbl_text_left.setFont(new Font("Nirmala UI Semilight", Font.PLAIN, 17));
		//lbl_text_left.setForeground(new Color(190,190,190));
		//lbl_text_left.setForeground(new Color(255,255,255));
		lbl_text_left.setHorizontalAlignment(SwingConstants.CENTER);
		lbl_text_left.setVerticalAlignment(SwingConstants.CENTER);
		btn_move_left.add(lbl_text_left);
		
	}
	
	private void initialEventsInSectionForm() {
		//Add text to form's title
		dialog.setTitle(dialog.TITLE_FORM_SECTION4);
		lbl_indications_form.setText(dialog.getTitleDialog());
		
		//Set inactive button 
		lbl_text_left.setForeground(new Color(166, 172, 175));
		lbl_text_left.setIcon(icon.arrow_left_inactive);
		
		//set form's section
		setSectionForm(1);
	}
	
	private void setSectionForm(int sec_form) {
		this.sec_form = sec_form;
	}
	
	private int getSectionForm() {
		return sec_form;
	}
	
	private void setMoveToRight(boolean move_to_right) {
		this.move_to_right = move_to_right;
	}
	
	private boolean getMoveToRight(){
		return move_to_right;
	}
	
	private void setMoveToLeft(boolean move_to_left) {
		this.move_to_left = move_to_left;
	}
	
	private boolean getMoveToLeft(){
		return move_to_left;
	}
	
	@SuppressWarnings("unchecked")
	private void addItemsComboBox() {
		int actual_year = Calendar.getInstance().get(Calendar.YEAR);
		String month[] = {"Enero","Febrero","Marzo","Abril","Mayo","Junio","Julio",
				"Agosto","Septiembre","Octubre","Noviembre","Diciembre"};
		
		for (int i = 1; i <= 31; i++) {
			box_day.addItem(makeObj(String.valueOf(i)));
		}
		for (int j = 0; j <= month.length-1; j++){
			box_month.addItem(makeObj(String.valueOf(month[j])));
		}
		for (int k = 1960; k <= actual_year-18; k++){
			box_year.addItem(makeObj(String.valueOf(k)));
		}
		
		//Initial values
		box_day.setSelectedIndex(0);
		box_month.setSelectedIndex(0);
		box_year.setSelectedIndex(0);
	}
	
	private Object makeObj(final String item) {
		return new Object() {
			public String toString() {
				return item;
			}
		};
	}
	
	private void addPlaceHolderList() {
		String items[] = {
				//for section1
				"Escriba su nombre(s)",
				"Escriba su apellido paterno",
				"Escriba su apellido materno",
				"Escriba su CURP",
				//for section2
				"Estado de residencia",
				"Ciudad ó municipio de residencia",
				"Nombre de la colonia",
				"Nombre de la calle",
				"CP de residencia",
				"No. Del domicilio",
				"No. Del domicilio",
				//for section3
				"Dirección de email",
				"Número teléfonico",
				"Número teléfonico"};
		
		for(int i = 0; i < items.length; i++) {
			this.placeholder.add(i, items[i]);
		}
		//for(int j = 4; j < 11 ; j++) { System.out.print(placeholder.get(j)); }
		setPlaceHolderInTextField();
	}
	
	private ArrayList<Object> getPlaceHolder() {
		return placeholder;
	}
	
	private void setPlaceHolderInTextField() {
		//section1
		txt_name.setText(placeholder.get(0).toString());
		txt_last_nameP.setText(placeholder.get(1).toString());
		txt_last_nameM.setText(placeholder.get(2).toString());
		txt_curp.setText(placeholder.get(3).toString());
		//section2
		txt_state.setText(placeholder.get(4).toString());
		txt_city.setText(placeholder.get(5).toString());
		txt_neighborhood.setText(placeholder.get(6).toString());
		txt_street.setText(placeholder.get(7).toString());
		txt_cp.setText(placeholder.get(8).toString());
		txt_ext_number.setText(placeholder.get(9).toString());
		txt_int_number.setText(placeholder.get(10).toString());
		//section3
		txt_email_address.setText(placeholder.get(11).toString());
		txt_main_phone_number.setText(placeholder.get(12).toString());
		txt_second_phone_number.setText(placeholder.get(13).toString());
	}
	
	private void changedSectionForm() {
		if(getMoveToRight()) {
			if(container_form_section1.isVisible()) {
				container_form_section1.setVisible(false);
				container_form_section2.setVisible(true);
				dialog.setTitle(dialog.TITLE_FORM_SECTION2);
				lbl_indications_form.setText(dialog.getTitleDialog());
				setDirectionButtons();
				setSectionForm(2);
			}else {
				if(container_form_section2.isVisible()) {
					container_form_section2.setVisible(false);
					container_form_section3.setVisible(true);
					dialog.setTitle(dialog.TITLE_FORM_SECTION3);
					lbl_indications_form.setText(dialog.getTitleDialog());
					setDirectionButtons();
					setSectionForm(3);
				}else {
					if(container_form_section3.isVisible()) {
						container_form_section3.setVisible(false);
						container_form_section4.setVisible(true);
						dialog.setTitle(dialog.TITLE_FORM_SECTION4);
						lbl_indications_form.setText(dialog.getTitleDialog());
						setDirectionButtons();
						setSectionForm(4);
					}
				}
			}
			
		}else {
			
			if(getMoveToLeft()) {
				if(container_form_section4.isVisible()) {
					container_form_section4.setVisible(false);
					container_form_section3.setVisible(true);
					dialog.setTitle(dialog.TITLE_FORM_SECTION3);
					lbl_indications_form.setText(dialog.getTitleDialog());
					setDirectionButtons();
					setSectionForm(2);
					if(container_form_section3.isVisible()) {
						container_form_section3.setVisible(false);
						container_form_section2.setVisible(true);
						dialog.setTitle(dialog.TITLE_FORM_SECTION2);
						lbl_indications_form.setText(dialog.getTitleDialog());
						setDirectionButtons();
						setSectionForm(2);
					}else {
						if(container_form_section2.isVisible()) {
							container_form_section2.setVisible(false);
							container_form_section1.setVisible(true);
							dialog.setTitle(dialog.TITLE_FORM_SECTION1);
							lbl_indications_form.setText(dialog.getTitleDialog());
							setDirectionButtons();
							setSectionForm(1);
						}
					}
				}
			}
		}
	}
	
	private boolean isInformationEmptyOrNull(int section) {
		ArrayList<Object> JSwingComponents = new ArrayList<Object>();
		//for section1
		JSwingComponents.add(txt_name.getText().toString());
		JSwingComponents.add(txt_last_nameP.getText().toString());
		JSwingComponents.add(txt_last_nameM.getText().toString());
		JSwingComponents.add(txt_curp.getText().toString());
		//for section2
		JSwingComponents.add(txt_state.getText().toString());
		JSwingComponents.add(txt_city.getText().toString());
		JSwingComponents.add(txt_neighborhood.getText().toString());
		JSwingComponents.add(txt_street.getText().toString());
		JSwingComponents.add(txt_cp.getText().toString());
		JSwingComponents.add(txt_ext_number.getText().toString());
		JSwingComponents.add(txt_int_number.getText().toString());
		//for section3
		JSwingComponents.add(txt_email_address.getText().toString());
		JSwingComponents.add(txt_main_phone_number.getText().toString());
		JSwingComponents.add(txt_second_phone_number.getText().toString());
		
		switch(section) {
		case 1:
			for(int i = 0; i < 4; i++) {
				if(!((JSwingComponents.get(i).equals(getPlaceHolder().get(i))) || (JSwingComponents.get(i).equals("")) || isBornDateCheckBoxEmpty())) {
					return false;
				}
			}
			break;
		case 2:
			for(int i = 4; i < 11; i++) {
				if(!((JSwingComponents.get(i).equals(placeholder.get(i))) || (JSwingComponents.get(i).equals("")))) {
					return false;
				}
			}
			break;
		case 3:
			for(int i = 11; i < 14; i++) {
				if(!((JSwingComponents.get(i).equals(placeholder.get(i))) || (JSwingComponents.get(i).equals("")))) {
					return false;
				}
			}
			break;
		default: System.out.print("Internal system error");
		}
		
		dialog.setTitle(dialog.TITLE_ERROR_IN_USER_REGISTER_FORM);
		dialog.setDialog(dialog.DIALOG_ERROR_IN_USER_REGISTER_FORM);
		return true;
	}
	
	private boolean isBornDateCheckBoxEmpty() {
		if(box_day.getSelectedIndex() == 0
				|| box_month.getSelectedIndex() == 0 
				|| box_year.getSelectedIndex() == 0) {
			return true;
		}
		return false;
	}
	
	private void setDirectionButtons() {
		if(!(container_form_section1.isVisible())) {
			//Set active button
			lbl_text_left.setForeground(foregroundTextStandard);
			lbl_text_left.setIcon(icon.arrow_left_exited);
		}else {
			//Set inactive button 
			lbl_text_left.setForeground(new Color(166, 172, 175));
			lbl_text_left.setIcon(icon.arrow_left_inactive);
		}
	}
	
	private void updateAdditionalEmail() {
		lbl_add_additional_email.setIcon(icon.UPDATE_ADDITIONAL_EMAIL_EXITED);
		lbl_add_additional_email.setText("Actualizar email adicional");
		
		lbl_add_additional_email.addMouseListener(new MouseAdapter() {
			@Override
			public void mouseClicked(MouseEvent e) {
				lbl_input_email_address.setBounds(20,0,200,40);
				txt_email_address.setBounds(20,41,320,30);
				lbl_set_additional_email.setText(additionalEmail.getAdditionalEmail());
			}
		});
	}
